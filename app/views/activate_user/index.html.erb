<%= render "layouts/menu" %>


<% @usuarios.each do |user| %>
  <div class="panel panel-success ">
    <div class="panel-heading">
      <h3 class="panel-title"><%= user.id%></h3>
    </div>
    <div class="panel-body">
		<h1><%= user.name%></h1>
		<h1><%= user.email%></h1>
		<h1><%= user.rol%></h1>
    </div>
  </div>
<% end %>


<%= render "layouts/footer" %>