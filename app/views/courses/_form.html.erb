<%= form_for(course) do |f| %>
  <% if course.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(course.errors.count, "error") %> prohibited this course from being saved:</h2>

      <ul>
      <% course.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div class="field">
    <%= f.label :profesor %><br />
    <%= f.select :users_id, User.all.map{|p| [p.name, p.id]} %>
  </div>

  <div class="field">
    <%= f.label :date_end %>
    <%= f.text_field :date_end %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
